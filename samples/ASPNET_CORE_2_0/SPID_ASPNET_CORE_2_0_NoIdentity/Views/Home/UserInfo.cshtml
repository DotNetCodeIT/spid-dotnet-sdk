@{
    ViewData["Title"] = "Contact";
}


<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Field</th>
            <th scope="col">Value</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Context.User.Claims)
    {
    <tr>
        <th scope="row">#</th>
        <td>@x.Type</td>
        <td>@x.Value</td>
      
    </tr>
     }
       
       
    </tbody>
</table>
